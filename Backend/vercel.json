{
  "version": 2,
  "builds": [{ "src": "/server.js", "use": "@vercel/node", "config": {
        "maxDuration": 60, 
        "memory": 3008, 
        "maxBodySize": "4.5mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ]
}
