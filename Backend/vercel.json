{
  "version": 2,
  "builds": [{ "src": "/server.js", "use": "@vercel/node" }],  "functions": {
    "server": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ]
}
